{
  "name": "react-native-snackbar-dialog",
  "version": "2.0.1-pre",
  "description": "A react native snackbar with dialog options",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src --out-dir dist --source-maps",
    "start": "node dist/index.js",
    "check": "npm run lint && npm run flow && npm test",
    "lint": "standard --fix",
    "test": "jest",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9gag-open-source/react-native-snackbar-dialog.git"
  },
  "keywords": [
    "snackbar",
    "react",
    "react-native"
  ],
  "author": "9gag Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/9gag-open-source/react-native-snackbar-dialog/issues"
  },
  "babel": {
    "presets": [
      "es2016",
      "react-native"
    ],
    "env": {
      "development": {
        "plugins": [
          "flow-react-proptypes"
        ]
      }
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "it",
      "expect"
    ]
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|static-container)"
    ]
  },
  "homepage": "https://github.com/9gag-open-source/react-native-snackbar-dialog#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-flow-react-proptypes": "^6.1.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "prop-types": "^15.6.0",
    "react-mixin": "^4.0.0",
    "react-native-root-siblings": "^1.2.1",
    "react-timer-mixin": "^0.13.3",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "enzyme": "^3.1.0",
    "flow-bin": "^0.56.0",
    "jest": "^21.2.1",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-native": "^0.49.3",
    "react-test-renderer": "^16.0.0",
    "standard": "^10.0.3"
  }
}
